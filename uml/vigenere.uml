        </ownedParameter>
        <ownedParameter xmi:id="_NSDWA0GPEeOSiMoPmtTPmg" name="texte">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
        </ownedParameter>
        <language>java</language>
        <body>String texteChiffre = &quot;&quot;;&#xD;
   int lgCle = chiffreCesar.size();&#xD;
	   for (int i=0; i&lt;texte.length(); i++){&#xD;
 	  char c = texte.charAt(i);&#xD;
 	  char cChiffre = chiffreCesar.get(i%lgCle).chiffrer(c);&#xD;
 	  texteChiffre += cChiffre;&#xD;
   }&#xD;
	   return texteChiffre;&#xD;
</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_NSDWBEGPEeOSiMoPmtTPmg" name="ChiffreVigenere" specification="_uNPPoEDJEeO8bIKpoVbFMg">
