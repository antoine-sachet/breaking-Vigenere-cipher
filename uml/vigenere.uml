      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_NSDWAUGPEeOSiMoPmtTPmg" name="chiffrer" specification="_5BXn8EDGEeO8bIKpoVbFMg">
        <ownedParameter xmi:id="_NSDWAkGPEeOSiMoPmtTPmg" direction="return">
          <name xsi:nil="true"/>
