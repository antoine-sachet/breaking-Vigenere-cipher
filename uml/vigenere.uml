        </ownedParameter>
        <language>java</language>
        <body>String texteDechiffre = &quot;&quot;;&#xD;
	      int lgCle = chiffreCesar.size();&#xD;
	      for (int i=0; i&lt;texte.length(); i++){&#xD;
	    	  char c = texte.charAt(i);&#xD;
	    	  char cDechiffre = chiffreCesar.get(i%lgCle).dechiffrer(c);&#xD;
